    "description": "User login/logout API with its resources",
    "license": "GPL-3.0-or-later",
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "autoload": {
        "psr-4": {
            "Mathleite\\PhpArch\\": "src/"
        }
    },
    "autoload-dev": {
